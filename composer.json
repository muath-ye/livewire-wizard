{
    "name": "muath-ye/livewire-wizard",
    "description": "Laravel Livewire Wizard (Multi-Step Form) component",
    "keywords": [
        "laravel",
        "livewire",
        "multi-steps",
        "wizard",
        "form"
    ],
    "homepage": "https://github.com/muath-ye/livewire-wizard",
    "license": "MIT",
    "authors": [{
        "name": "Vildan Bina",
        "email": "vildanbina@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^2.0",
        "wireui/wireui": "^1.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Muathye\\LivewireWizard\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muathye\\LivewireWizard\\LivewireWizardServiceProvider"
            ],
            "aliases": {
                "LivewireWizard": "Muathye\\LivewireWizard\\Facades\\LivewireWizard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
